            OUTFILE= "Z:\CDC\Analysis\001 FIRST LOOK\SAS Output (SUA001)\Verwendung der Variablen für Nicht rechnungswirksame Rabatte.xlsx" 
            DBMS=EXCEL REPLACE;
     		SHEET="av price by product name"; 
